{
  "name": "@jollywise/jollygoodgame",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "jgg": "buildtools/scripts/jgg.js"
  },
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "build:debug": "cross-env NODE_ENV=development webpack --config ./buildtools/jollygoodgame.webpack.config.js",
    "build:release": "cross-env NODE_ENV=production webpack --config ./buildtools/jollygoodgame.webpack.config.js",
    "jsdoc": "jsdoc -c ./buildtools/jollygoodgame.jsdocs.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jollywise/jollygoodgame.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jollywise/jollygoodgame/issues"
  },
  "homepage": "https://github.com/jollywise/jollygoodgame#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@pbs/eslint-config-pbs-kids": "^1.0.2",
    "autoprefixer": "^9.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^3.1.3",
    "cosmiconfig": "^7.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.3.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "ink-docstrap": "^1.3.2",
    "inline-manifest-webpack-plugin": "^4.0.1",
    "jsdoc": "^3.6.6",
    "jsdocs": "^1.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^2.1.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "dat.gui": "^0.7.6",
    "phaser": "^3.24.1",
    "webfontloader": "^1.6.26"
  },
  "prettier": {
    "printWidth": 100,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": false,
    "requirePragma": false,
    "arrowParens": "always"
  },
  "browserslist": [
    "last 1 version",
    "ie >= 11",
    "> 1%",
    "iOS >= 9",
    "android >= 4.4",
    "not dead"
  ],
  "eslintIgnore": [
    "node_modules"
  ],
  "eslintConfig": {
    "extends": [
      "prettier",
      "plugin:compat/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true,
        "arrowFunctions": true,
        "restParams": true,
        "experimentalObjectRestSpread": true
      }
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "compat/compat": "off",
      "no-console": "off",
      "new-cap": "off",
      "max-statements": "off"
    },
    "globals": {
      "__DEBUG__": false,
      "__DEBUG_BOUNDS__": false,
      "__WATCH__": false,
      "__SHORTCUTS_ENABLED__": false,
      "Phaser": false
    }
  }
}
